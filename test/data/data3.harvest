<?xml version="1.0"?>
<mws:harvest xmlns:mws="http://search.mathweb.org/ns" xmlns:m="http://www.w3.org/1998/Math/MathML">
  <mws:data mws:data_id="a">
    <foo>bar</foo>
  </mws:data>
  <mws:expr
    mws:data_id="a"
    url="foo!">
        <m:apply>
          <m:apply>
            <m:csymbol cd="ambiguous">subscript</m:csymbol>
            <m:limit/>
            <m:apply>
              <m:ci>&#x2192;</m:ci>
              <m:ci>x</m:ci>
              <m:cn>0</m:cn>
            </m:apply>
          </m:apply>
          <m:apply>
            <m:divide/>
            <m:cn>1</m:cn>
            <m:apply>
              <m:csymbol cd="ambiguous">superscript</m:csymbol>
              <m:ci>x</m:ci>
              <m:cn>2</m:cn>
            </m:apply>
          </m:apply>
        </m:apply>
  </mws:expr>
</mws:harvest>
